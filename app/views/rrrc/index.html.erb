<h2>Run Rrrc script</h2>
<%= form_for @form, :url => {:action => "req", :issue => @form.issue}  do |f| %>
  <div class="extract iss">
    Run test script file <%= link_to "#" + @form.id.to_s, 'http://www.google.co.jp' %> to <%= f.select :ip, @ip_table%>
  </div>
  <h3>Details</h3>
  <p><%= f.submit "Run" %>
    <% if @form.issue %>
      <%= link_to "back to ticket", {controller: 'issues', action: 'show', id: @form.issue } %>
    <% end %>
  </p>
<% end %>
